	#bar li a.active{ color: #999999; font-size: 12px; }
	#bar li a:hover { text-decoration: none; color: #c5e953; text-decoration:none; border-bottom: 3px solid #c5e953; }
	

div.searchoption{padding:8px; border:solid 1px #CCCCCC; width:90%; margin-left:2px;}
div.suboption{margin-left:14px; margin-bottom:10px;}
div.suboption select{display:block; width:100%}

